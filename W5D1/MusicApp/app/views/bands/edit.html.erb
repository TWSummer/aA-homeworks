<h1>Edit Band</h1>

<form action="<%= band_url(@band) %>" method="post">
  <%= authenticate %>
  <input type="hidden" name="_method" value="PATCH">
  <label for="name">Band Name: </label>
  <input type="text" name="band[name]" value="<%= @band.name %>" id="name">
  <br>
  <input type="submit" value="Update">
</form>
